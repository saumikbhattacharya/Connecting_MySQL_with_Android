<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="saubhattacharya.stationarymanager.com">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/manager_stationery_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Inventory_List"
            android:label="@string/title_activity_inventory__list"
            android:parentActivityName=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="saubhattacharya.stationarymanager.com.LoginActivity" />
        </activity>
        <activity
            android:name=".New_User_Registration"
            android:label="@string/title_activity_new__user__registration"
            android:parentActivityName=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="saubhattacharya.stationarymanager.com.LoginActivity" />
        </activity>
        <activity
            android:name=".Inventory_List_Emp"
            android:label="@string/title_activity_inventory__list__emp"
            android:parentActivityName=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="saubhattacharya.stationarymanager.com.LoginActivity" />
        </activity>
        <activity
            android:name=".Edit_Item"
            android:label="@string/title_activity_edit__item"
            android:parentActivityName=".Inventory_List"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="saubhattacharya.stationarymanager.com.Inventory_List" />
        </activity>
        <activity
            android:name=".Add_Item"
            android:label="@string/title_activity_add__item"
            android:parentActivityName=".Inventory_List"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="saubhattacharya.stationarymanager.com.Inventory_List" />
        </activity>
        <activity
            android:name=".Select_Item"
            android:label="@string/title_activity_select__item"
            android:parentActivityName=".Inventory_List_Emp"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="saubhattacharya.stationarymanager.com.Inventory_List_Emp" />
        </activity>
        <activity
            android:name=".Export"
            android:label="@string/title_activity_export"
            android:parentActivityName=".Inventory_List"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="saubhattacharya.stationarymanager.com.Inventory_List" />
        </activity>
        <activity
            android:name=".Show_Cart"
            android:label="@string/title_activity_show__cart"
            android:parentActivityName=".Inventory_List_Emp"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="saubhattacharya.stationarymanager.com.Inventory_List_Emp" />
        </activity>
        <activity
            android:name=".AboutUs"
            android:label="@string/title_activity_about_us"
            android:parentActivityName=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="saubhattacharya.stationarymanager.com.LoginActivity" />
        </activity>
    </application>

</manifest>
